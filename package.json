{
  "name": "aton",
  "version": "3.0.0",
  "description": "ATON Framework",
  "main": "ATON.service.main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node services/ATON.service.main.js",
    "start-vrc": "node services/ATON.service.vroadcast.js",
    "deploy": "pm2 start ecosystem.config.js",
    "update-aton": "git stash && git pull && npm update",
    "build-three": "parcel build ./services/tools/three.bundle.js -d ./public/dist/ --out-file THREE.bundle.js",
    "build-aton": "parcel build ./public/src/ATON.js -d ./public/dist/ --out-file ATON.min.js",
    "build-apidoc": "jsdoc -c ./services/tools/jsdoc-client.json",
    "build-apidoc-server": "apidoc -c ./services/tools/apidoc-server.json -i ./services/ -o ./public/apidoc/server/ -e _prv -t ./_prv/apidoc-template/template"
  },
  "author": "Bruno Fanini",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/phoenixbf/aton.git"
  },
  "browserslist": [
    "since 2017-06"
  ],
  "dependencies": {
    "3d-tiles-renderer": "^0.3.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "child_process": "^1.0.2",
    "chokidar": "^3.5.2",
    "clamp": "^1.0.1",
    "command-line-args": "^4.0.7",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-session": "^1.17.1",
    "fast-glob": "^3.2.7",
    "fast-json-patch": "^3.1.0",
    "fs": "0.0.1-security",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.7",
    "gltf-bounding-box": "^0.4.1",
    "gltf-pipeline": "^3.0.4",
    "http": "0.0.0",
    "http-proxy-middleware": "^1.0.6",
    "https": "^1.0.0",
    "image-size": "^0.9.3",
    "jimp": "^0.16.1",
    "key-del": "^1.3.0",
